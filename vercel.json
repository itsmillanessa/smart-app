{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NOTION_TOKEN": "@notion_token",
    "NOTION_DATABASE_ID": "@notion_database_id", 
    "OPENAI_API_KEY": "@openai_api_key",
    "GOOGLE_API_KEY": "@google_api_key",
    "GOOGLE_SEARCH_ENGINE_ID": "@google_search_engine_id"
  }
}
